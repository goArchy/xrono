.week_pagination
  = render 'dashboard/base/week_pagination', :start_date => @start_date

= render 'shared/calendar', :start_date => @start_date, :user => current_user, :work_units => @work_units

%ul.nav.nav-tabs{"data-tabs" => "tabs"}
  %li.active= link_to "Add Work Unit", "#add_work_unit"
  %li= link_to "Add Ticket", "#add_ticket"

.tab-content
  #add_work_unit.tab-pane.active
    = render 'work_units/add_work_unit'
  #add_ticket.tab-pane
    = render 'tickets/add_ticket'

.dashboard_2week_time_report
  = render 'two_week_time_report'

= render_message(@message)

- content_for :javascripts do
  = javascript_include_tag 'add_taxonomy'
  = javascript_include_tag "bootstrap-tabs"
  :javascript
    $(function() {
      $('.tabs').tabs();
    });
